<div class="app-content content ">
  <div class="content-overlay"></div>
  <div class="header-navbar-shadow"></div>
  <div class="content-wrapper">
    <div class="content-header row">
    </div>
    
    <div class="card">
<div class="card-body">
            <form class="needs-validation was-validated" action="/save_vehiculos_py4_edit" method="post">
            <input  type="text" name="id_vehiculo" placeholder="Pedro" aria-describedby="register-firstName"
                  tabindex="1" autofocus="" class="form-control mb-2" value="{{vehiculos_let.id}}"
                  hidden>
              <div class="form-group"><label for="register-firstName" class="form-label">Matrícula</label>
              <input
                  type="text" name="matricula" placeholder="Matricula" aria-describedby="register-firstName"
                  tabindex="1" autofocus="" class="form-control mb-2"
                   value="{{vehiculos_let.matricula}}"></div>
              <div class="form-group"><label for="register-lastName" class="form-label">Marca</label><input
                  type="text" name="marca" placeholder="Marca" aria-describedby="register-lastName"
                  tabindex="2" class="form-control mb-2" value="{{vehiculos_let.marca}}"
                 ></div>
              
              <div class="form-group"><label for="register-direccion"
                  class="form-label">Módelo</label><input type="text" name="modelo"
                  placeholder="Módelo" aria-describedby="register-direccion" tabindex="5"
                  class="form-control mb-2" value="{{vehiculos_let.modelo}}"
                 ></div>

                 <div class="form-group"><label for="register-direccion"
                  class="form-label">Año</label><input type="text" name="anio"
                  placeholder="Año" aria-describedby="register-direccion" tabindex="5"
                  class="form-control mb-2" value="{{vehiculos_let.anio}}"
                 ></div>
                  <div class="form-group"><label for="register-direccion"
                  class="form-label">Tipo</label><input type="text" name="tipo"
                  placeholder="Tipo" aria-describedby="register-direccion" tabindex="5"
                  class="form-control mb-2" value="{{vehiculos_let.tipo}}"
                 ></div>
                  <div class="form-group"><label for="register-direccion"
                  class="form-label">Capacidad</label><input type="number" name="capacidad"
                  placeholder="Capacidad" aria-describedby="register-direccion" tabindex="5"
                  class="form-control mb-2" value="{{vehiculos_let.capacidad}}"
                 ></div>
                 
              <div class="form-group"><label for="register-ciudad" class="form-label">Status</label><select
                  name="status" placeholder="Status" aria-describedby="register-cargo" tabindex="3"
                  class="form-control mb-2"                 >
                  <option selected=""> {{vehiculos_let.status}} </option>
                  <option > Operativo </option>
                  <option> Inoperativo </option>

                </select></div>             
              <div class="form-group"><label for="register-sucursal"
                  class="form-label">Sucursal</label><select aria-describedby="register-sucursal" tabindex="24"
                  name="sucursal" class="form-control mb-2" id="sucursal_select">
                  {{#each sucursales_let as |sucursal i|}}
                  <option value={{id}}> {{nombre}} </option>
                  {{/each}}
                 
                </select></div>

          </div>
          <div class="modal-footer">
             <button tabindex="26" class="btn btn-primary btn-block waves-effect waves-float waves-light">Guardar</button>
          </div>
          </form>
        </div>
</div>
</div>




