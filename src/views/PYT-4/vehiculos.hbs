<style>
  .none {
    display: none;
  }
  @media(min-width: 768px) {
       table.dataTable.table-responsive:not(.datatables-ajax) {
    display: table !important;
}
    }
</style>

{{#if msg}}
<label id="msgs">{{msg}}</label>
{{/if}}
<input type="text" id="array_vehiculos" hidden value="{{vehiculos_}}">
<!-- BEGIN: Content-->
<div class="app-content content ">
  <div class="content-overlay"></div>
  <div class="header-navbar-shadow"></div>
  <div class="content-wrapper">
    <div class="content-header row">
    </div>
    <div class="content-body">
      <section id="basic-datatable">
        <div class="row">
          <div class="scrolling-inside-modal">

            <!-- Button trigger modal -->
            <button type="button" class="btn btn-outline-primary text-primary waves-effect mb-2" data-bs-toggle="modal"
              data-bs-target="#addEmployed">
              Agregar
            </button>



          </div>
          <div class="col-12">

            <div class="card">

              <div id="DataTables_Table_0_wrapper" class="dataTables_wrapper dt-bootstrap5 no-footer">
              </div>
              <div class="card-body">
                <h4 class="card-title">Vehículos</h4>
                <p class="card-text"> Todo los vehículos de la plataforma están acá, pronto podrás filtrarlos y
                  modificarlos rápidamente. </p>
              </div>
              <div class="d-flex justify-content-between align-items-center mx-0 row">

              </div>
              <!--<div class="d-flex justify-content-between align-items-center mx-0 row none"><div class="col-sm-12 col-md-6"><div class="dataTables_length" id="DataTables_Table_0_length"><label>Show <select name="DataTables_Table_0_length" aria-controls="DataTables_Table_0" class="form-select"><option value="7">7</option><option value="10">10</option><option value="25">25</option><option value="50">50</option><option value="75">75</option><option value="100">100</option></select> entries</label></div></div><div class="col-sm-12 col-md-6"></div></div>-->

              <div class="table-responsive">
                <section id="basic-datatable">
                  <div class="row">
                    <div class="col-12">
                      <div class="card">
                        <table class="datatables-basic_vehiculos table table-responsive">
                          <thead>
                            <tr>
                              <th>id</th>
                              <th>Fecha Registro</th>
                              <th>Matricula</th>
                              <th>Marca</th>
                              <th>Modelo</th>
                              <th>Año</th>
                              <th>Tipo</th>
                              <th>Capacidad</th>
                              <th>Status</th>
                              <th>Sucursal</th>
                              <th>Opciones</th>
                            </tr>
                          </thead>
                        </table>
                      </div>
                    </div>
                  </div>
                </section>
              </div>

            </div>
          </div>
        </div>
    </div>


    <!-- Modal to add new record -->
    <!-- Modal -->
    <div class="modal fade" id="addEmployed" tabindex="-1" aria-labelledby="addEmployedTitle" style="display: none;"
      aria-hidden="true">
      <div class="modal-dialog modal-dialog-scrollable">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="addEmployedTitle">Agregar Vehículo</h5>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body">
            <form class="needs-validation was-validated" id="save_vehiculo_py4" method="post">
              <div class="form-group"><label for="register-firstName" class="form-label">Matrícula</label>
                <input type="text" name="matricula" placeholder="Matricula" aria-describedby="register-firstName"
                  tabindex="1" autofocus="" class="form-control mb-2" required>
              </div>
              <div class="form-group"><label for="register-lastName" class="form-label">Marca</label><input type="text"
                  name="marca" placeholder="Marca" aria-describedby="register-lastName" tabindex="2"
                  class="form-control mb-2"></div>

              <div class="form-group"><label for="register-direccion" class="form-label">Módelo</label><input
                  type="text" name="modelo" placeholder="Módelo" aria-describedby="register-direccion" tabindex="5"
                  class="form-control mb-2"></div>

              <div class="form-group"><label for="register-direccion" class="form-label">Año</label><input type="text"
                  name="anio" placeholder="Año" aria-describedby="register-direccion" tabindex="5"
                  class="form-control mb-2"></div>
              <div class="form-group"><label for="register-direccion" class="form-label">Tipo</label><input type="text"
                  name="tipo" placeholder="Tipo" aria-describedby="register-direccion" tabindex="5"
                  class="form-control mb-2"></div>
              <div class="form-group"><label for="register-direccion" class="form-label">Capacidad</label><input
                  type="number" name="capacidad" placeholder="Capacidad" aria-describedby="register-direccion"
                  tabindex="5" class="form-control mb-2"></div>

              <div class="form-group"><label for="register-ciudad" class="form-label">Status</label><select
                  name="status" placeholder="Status" aria-describedby="register-cargo" tabindex="3"
                  class="form-control mb-2">
                  <option selected=""> Operativo </option>
                  <option> Inoperativo </option>

                </select></div>
              <div class="form-group"><label for="register-sucursal" class="form-label">Sucursal</label><select
                  aria-describedby="register-sucursal" tabindex="24" name="sucursal" class="form-control mb-2">
                  <option selected=""> Seleccione una sucursal </option>
                  {{#each sucursales_let as |sucursal i|}}
                  <option value={{id}}> {{nombre}} </option>
                  {{/each}}
                </select></div>
          </div>
          <div class="modal-footer">
            <button tabindex="26" class="btn btn-primary btn-block waves-effect waves-float waves-light"
              id="reg_vehiculos" type="button">Regístrar</button>
          </div>
          </form>
        </div>
      </div>
    </div>

    <div class="modal fade" id="edit_veh" tabindex="-1" aria-labelledby="edit_vehTitle" style="display: none;"
      aria-hidden="true">
      <div class="modal-dialog modal-dialog-scrollable">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="edit_vehTitle">Editar Vehículo</h5>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body">
            <form class="needs-validation was-validated" id="save_vehiculos_py4_edit" method="post">
              <input type="text" name="id_vehiculo" placeholder="Pedro" id="id_vehiculo" hidden>
              <div class="form-group">
                <label for="register-firstName" class="form-label">Matrícula</label>
                <input type="text" name="matricula" placeholder="Matricula" aria-describedby="register-firstName"
                  tabindex="1" autofocus="" class="form-control mb-2" id="edit_matricula" required>
              </div>

              <div class="form-group">
                <label for="register-lastName" class="form-label">Marca</label>
              <input type="text"
                  name="marca" placeholder="Marca" class="form-control mb-2" id="edit_marca">
              </div>

              <div class="form-group">
                <label for="register-direccion" class="form-label">Módelo</label>
              <input
                  type="text" name="modelo" placeholder="Módelo" aria-describedby="register-direccion" tabindex="5"
                  class="form-control mb-2" id="edit_modelo">
              </div>

              <div class="form-group">
                <label for="register-direccion" class="form-label">Año</label>
              <input type="text"
                  name="anio" placeholder="Año" aria-describedby="register-direccion" tabindex="5"
                  class="form-control mb-2" id="edit_anio">
              </div>
              <div class="form-group">
                <label for="register-direccion" class="form-label">Tipo</label>
              <input type="text" name="tipo" placeholder="Tipo" id="edit_tipo" class="form-control mb-2">
                  </div>
              <div class="form-group"><label for="register-direccion" class="form-label">Capacidad</label><input
                  type="number" name="capacidad" placeholder="Capacidad" id="edit_capacidad" class="form-control mb-2"></div>

              <div class="form-group"><label for="register-ciudad" class="form-label">Status</label><select
                  name="status" placeholder="Status" id="edit_status"
                  class="form-control mb-2">
                  <option selected="" value="Operativo">Operativo</option>
                  <option value="Inoperativo">Inoperativo</option>

                </select></div>
              <div class="form-group"><label for="register-sucursal" class="form-label">Zona</label>
              <select id="edit_zona" name="sucursal" class="form-control mb-2">
                  <option selected=""> Seleccione una sucursal </option>
                  {{#each sucursales_let as |sucursal i|}}
                  <option value={{id}}> {{nombre}} </option>
                  {{/each}}
                </select></div>
          </div>
          <div class="modal-footer">
            <button tabindex="26" class="btn btn-primary btn-block waves-effect waves-float waves-light"
              id="edit_vehiculosbtn" type="button">Guardar</button>
          </div>
          </form>
        </div>
      </div>
    </div>

    </section>
  </div>
</div>
</div>
</div>
</div>




<!-- END: Content-->